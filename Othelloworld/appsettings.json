{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OthelloWorldDatabase": "Server=127.0.0.1;Database={{database_name}};User id=sa;Password={{database_password}};MultipleActiveResultSets=true"
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "ReactAspAuth": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "Issuer": "{{website_url}}",
  "Audience": "{{audience}}",
  "EncryptionKey": "{{encryption_key}}",
  "SigningKey": "{{signing_key}}",
  "TokenTimeoutMinutes": 60,
  "SecretCaptchaKey": "{{google_captcha_key}}"
}
