{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OthelloWorldDatabase": "Server=(localdb)\\mssqllocaldb;Database={{database_name}};Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "ReactAspAuth": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  //"Authority": "https://127.0.0.1:5000",
  "Issuer": "https://127.0.0.1:5000",
  "Audience": "{{audience}}",
  "EncryptionKey": "{{encryption_key}}",
  "SigningKey": "{{signing_key}}",
  "TokenTimeoutMinutes": 60,
  "SecretCaptchaKey": "{{google_captcha_key}}"
}
